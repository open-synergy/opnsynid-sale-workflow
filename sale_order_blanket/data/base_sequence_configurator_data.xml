<openerp>
  <data>
    <record id="base_sequence_sale_order_blanket" model="base.sequence_configurator">
        <field name="model_id" ref="sale_order_blanket.model_sale_order_blanket"/>
        <field name="sequence_field_id" search="[('model_id.model','=','sale.order.blanket'),('name','=','name')]"/>
        <field name="fallback_sequence_id" ref="sale_order_blanket.sequence_sale_order_blanket"/>
    </record>

    <record id="base_sequence_sale_order_blanket_line" model="base.sequence_configurator_line">
        <field name="generator_id" ref="sale_order_blanket.base_sequence_sale_order_blanket"/>
        <field name="sequence" eval="1"/>
        <field name="sequence_computation_code">result = document.type_id.blanket_sequence_id</field>
    </record>
  </data>
</openerp>
